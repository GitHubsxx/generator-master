<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="true">
    <!--年度 -->
    <class name="com.bwol.adultEdu.enroll.entity.Annual" table="T_Annual">
    		<id name="id" type="java.lang.Long">
			<column name="ANNUAL_ID" precision="16" scale="0"/>
			<generator class="native" />
		</id>

		<property generated="never" lazy="false" name="title" type="java.lang.String">
			<column length="20" name="TITLE" />
		</property>

		<property generated="never" lazy="false" name="simpleTitle" type="java.lang.String">
			<column length="20" name="SIMPLE_TITLE" />
		</property>

		<property generated="never" lazy="false" name="isEnroll" type="java.lang.Integer">
			<column length="20" name="IS_ENROLL" />
		</property>

		<property generated="never" lazy="false" name="createUserId" type="java.lang.Long">
			<column length="20" name="CREATE_USER_ID" />
		</property>

		<property generated="never" lazy="false" name="createUserName" type="java.lang.String">
			<column length="20" name="CREATE_USER_NAME" />
		</property>

		<property generated="never" lazy="false" name="createTime" type="java.util.Date">
			<column length="20" name="CREATE_TIME" />
		</property>

		<property generated="never" lazy="false" name="updateUserId" type="java.lang.Long">
			<column length="20" name="UPDATE_USER_ID" />
		</property>

		<property generated="never" lazy="false" name="updateUserName" type="java.lang.String">
			<column length="20" name="UPDATE_USER_NAME" />
		</property>

		<property generated="never" lazy="false" name="updateTime" type="java.util.Date">
			<column length="20" name="UPDATE_TIME" />
		</property>


    </class>

</hibernate-mapping>