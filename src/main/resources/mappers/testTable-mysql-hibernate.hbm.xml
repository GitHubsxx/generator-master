<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="true">
    <!--测试 -->
    <class name="com.bwol.adultEdu.base.entity.TestTable" table="T_BASE_TESTTABLE">
    		<id name="id" type="java.lang.Long">
			<column name="ID" precision="16" scale="0"/>
			<generator class="native" />
		</id>

		<many-to-one name="schoolId"  class="com.bwol.adultEdu.base.entity.School"  fetch="select">
			<column name="SCHOOL_ID" precision="16" scale="0" />
		</many-to-one>
		<property generated="never" lazy="false" name="name" type="java.lang.String">
			<column length="10" name="NAME" />
		</property>

		<property generated="never" lazy="false" name="size" type="java.lang.Integer">
			<column length="11" name="SIZE" />
		</property>


    </class>

</hibernate-mapping>