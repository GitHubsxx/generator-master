<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="true">
    <!--学校 -->
    <class name="com.bwol.adultEdu.base.entity.School" table="T_BASE_SCHOOL">
    		<id name="id" type="java.lang.Long">
			<column name="SCHOOL_ID" precision="16" scale="0"/>
			<generator class="native" />
		</id>

		<property generated="never" lazy="false" name="title" type="java.lang.String">
			<column length="100" name="TITLE" />
		</property>

		<property generated="never" lazy="false" name="address" type="java.lang.String">
			<column length="200" name="ADDRESS" />
		</property>

		<property generated="never" lazy="false" name="runSchoolForm" type="java.lang.String">
			<column length="100" name="RUN_SCHOOL_FORM" />
		</property>

		<property generated="never" lazy="false" name="runSchoolType" type="java.lang.String">
			<column length="100" name="RUN_SCHOOL_TYPE" />
		</property>

		<property generated="never" lazy="false" name="approvalNumber" type="java.lang.String">
			<column length="100" name="APPROVAL_NUMBER" />
		</property>

		<property generated="never" lazy="false" name="planProperties" type="java.lang.String">
			<column length="200" name="PLAN_PROPERTIES" />
		</property>

		<property generated="never" lazy="false" name="createUserName" type="java.lang.String">
			<column length="100" name="CREATE_USER_NAME" />
		</property>

		<property generated="never" lazy="false" name="createUserId" type="java.lang.Long">
			<column length="20" name="CREATE_USER_ID" />
		</property>

		<property generated="never" lazy="false" name="updateUserName" type="java.lang.String">
			<column length="100" name="UPDATE_USER_NAME" />
		</property>

		<property generated="never" lazy="false" name="updateUserId" type="java.lang.Long">
			<column length="20" name="UPDATE_USER_ID" />
		</property>


    </class>

</hibernate-mapping>